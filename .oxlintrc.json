{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "plugins": ["import"],
  "rules": {
    "eqeqeq": "error",
    "consistent-type-imports": ["warn", {
      "prefer": "type-imports",
      "fixStyle": "inline-type-imports"
    }],
    "import/consistent-type-specifier-style": ["warn", "prefer-inline"]
  }
}
